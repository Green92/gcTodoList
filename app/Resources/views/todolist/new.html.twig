{% extends 'base.html.twig' %}

{% block body %}
    <h1>TodoList creation</h1>
    {% form_theme form 'bootstrap_3_layout.html.twig' %}
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="Create" class="btn btn-default pull-right" />
    {{ form_end(form) }}

    <div class="clearfix">
        <a href="{{ path('todolist_index') }}" class="btn btn-default pull-right">Back to the list</a>
    </div>
{% endblock %}
